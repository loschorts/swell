<!DOCTYPE html>
<html>
<head>
  <title>SampleProjectProposal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= "signed in as #{ current_user ? current_user.username : 'nobody'}" %>
<% if current_user %>
  <%= button_to "Logout", session_url, method: :delete %>
<% end %>
<%= flash[:errors] %>
<%= yield %>



</body>
</html>
